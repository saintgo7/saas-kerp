# Tax Scraper Service Environment Configuration

# Service
ENV=development
DEBUG=false
LOG_LEVEL=INFO
LOG_FORMAT=json

# gRPC Server
GRPC_HOST=0.0.0.0
GRPC_PORT=50051
GRPC_MAX_WORKERS=10
GRPC_REFLECTION_ENABLED=true

# Hometax Configuration
HOMETAX_BASE_URL=https://www.hometax.go.kr
HOMETAX_TIMEOUT=30
HOMETAX_RETRY_COUNT=3
HOMETAX_RETRY_DELAY=1.0

# Browser (Playwright)
BROWSER_HEADLESS=true
BROWSER_SLOW_MO=0
BROWSER_TIMEOUT=30000

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# Encryption (SEED for Korean government systems)
SEED_ENCRYPTION_KEY=
